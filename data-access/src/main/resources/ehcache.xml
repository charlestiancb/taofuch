<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="true"
	monitoring="autodetect" dynamicConfig="true">
	<diskStore path="java.io.tmpdir" />
	<cache name="defaultCache" maxEntriesLocalHeap="1000000" eternal="true"
		timeToIdleSeconds="360000000" timeToLiveSeconds="360000000"
		diskSpoolBufferSizeMB="30" maxEntriesLocalDisk="2147483646"
		diskExpiryThreadIntervalSeconds="360000000" memoryStoreEvictionPolicy="LRU"
		statistics="false">
		<persistence strategy="localTempSwap" />
	</cache>
</ehcache>